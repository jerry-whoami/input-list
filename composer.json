{
  "name": "jerry-whoami/input-list",
  "description": "Text input with a select box option for filamentphp",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "filament/filament": "^2.0",
    "spatie/laravel-package-tools": "^1.9.2"
  },
  "autoload": {
    "psr-4": {
      "JerryWhoami\\InputList\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JerryWhoami\\InputList\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "JerryWhoami\\InputList\\InputListServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "JerryWhoami",
      "email": "jerry.whoami@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "orchestra/testbench": "^8.5"
  }
}
